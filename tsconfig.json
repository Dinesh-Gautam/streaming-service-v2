{
  "compilerOptions": {
    "target": "es2021",
    "module": "nodenext",
    "lib": [
      "es2021",
      "dom"
    ],
    "strict": true,
    "declaration": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "moduleResolution": "nodenext",
  }
}